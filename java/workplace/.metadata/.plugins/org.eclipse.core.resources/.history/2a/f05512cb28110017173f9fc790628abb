import java.util.Scanner;

public class test6 {

	public static void main(String[] args) {
		Scanner cin = new Scanner(System.in);
		String exp = cin.nextLine();
		int result = Integer.parseInt(String.valueOf(exp.charAt(0)));
		for(int i=1;i<exp.length();i++){
            if(String.valueOf(exp.charAt(i)).equals("+")){
            	result += Integer.parseInt(String.valueOf(exp.charAt(i+1)));
            	i++;
            }else if(String.valueOf(exp.charAt(i)).equals("-")){
            	result -= Integer.parseInt(String.valueOf(exp.charAt(i+1)));
            	i++;
            }else if(String.valueOf(exp.charAt(i)).equals("*")){
            	result *= Integer.parseInt(String.valueOf(exp.charAt(i+1)));
            	i++;
            }
        }
	}

}
