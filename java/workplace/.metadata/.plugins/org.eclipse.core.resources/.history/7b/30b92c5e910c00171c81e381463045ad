import java.util.Scanner;

public class test {
	public static void main(String[] args) {
		Scanner cin = new Scanner(System.in);
		double m = cin.nextInt();
		int n = cin.nextInt();
		double sum = 0;
		
		for(int i=0;i<n;i++){
			sum = sum + m;
			m = Math.sqrt(m);
		}
		
		System.out.println(sum);
	}
}
